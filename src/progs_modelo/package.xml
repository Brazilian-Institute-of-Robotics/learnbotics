<?xml version="1.0"?>
<package format="2">
  <name>progs_modelo</name>
  <version>0.0.0</version>
  <description>Pacote que contem todos os programas modelo do projeto Learnbotics</description>

  <!-- MAINTAINERS -->
  <maintainer email="nozela.leo@gmail.com">Leandro Nozela</maintainer>


  <!-- LICENSES -->
  <license>BSD</license>


  <!-- Url tags are optional, but multiple are allowed, one per tag -->
